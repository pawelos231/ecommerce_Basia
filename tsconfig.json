{
  "compilerOptions": {
    "target": "esnext",
    "moduleResolution": "node",
    "lib": [
      "es6",
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "isolatedModules": true,
    "jsx": "preserve",
    "noImplicitAny": true,
     "noImplicitThis": true,
     "strictNullChecks": true,
    "resolveJsonModule": true
  },
  "include": [
    "next-env.d.ts",
    "pages/**/*",
    "src/**/*",
    "**/*.ts",
    "**/*.tsx",
    "pages/404.js"
, "components/Navbar/Navbar.js"  ],
  "exclude": [
    "node_modules"
  ]
}
